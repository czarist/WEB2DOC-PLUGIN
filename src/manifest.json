{
  "manifest_version": 3,
  "name": "Web2Doc - Intelligent Converter",
  "version": "1.0",
  "description": "Exports tables from web pages to Excel and converts pages to PDF",
  "permissions": ["activeTab", "storage", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }]
}